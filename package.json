  "bugs": {
    "url": "https://github.com/hyperaudio/hyperaudio-pad/issues"
  },
  "browserslist": [
    "> 1%"
  ],
  "homepage": "https://github.com/hyperaudio/hyperaudio-pad#readme",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "debug": "^2.6.8"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-flow": "^6.23.0",
    "cross-env": "^5.0.1",
    "eslint": "^3.19.0",
